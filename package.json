{
  "name": "@lukutpg/lando-magento",
  "description": "A Lando plugin that provides a tight integration with Magento.",
  "version": "1.10.0",
  "author": "Pavel Lukuts",
  "license": "MIT",
  "repository": "lando/symfony",
  "bugs": "https://github.com/lukutpg/lando-magento/issues/new/choose",
  "homepage": "https://github.com/lukutpg/lando-magento",
  "keywords": [
    "lando",
    "lando-plugin",
    "lando-recipe",
    "magento"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "lando": {},
  "main": "index.js",
  "nyc": {
    "include": [
      "lib/**/*.js",
      "recipes/**/*.js",
      "services/**/*.js",
      "types/**/*.js"
    ],
    "exclude": [
      "test/**"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs:build": "LANDO_MVB_VERSION=$(git describe --tags --always --abbrev=1 --match=\"v[0-9].*\") vitepress build docs",
    "docs:dev": "LANDO_MVB_VERSION=$(git describe --tags --always --abbrev=1 --match=\"v[0-9].*\") vitepress dev docs",
    "docs:mvb": "npx mvb docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint . --ext .js --ext .mjs ",
    "lint:fix": "eslint . --ext .js --ext .mjs --fix",
    "test:unit": "nyc --reporter=html --reporter=text mocha --timeout 5000 test/**/*.spec.js",
    "test:leia": "leia \"examples/**/README.md\" -c 'Destroy tests' --stdin",
    "test": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "@lando/elasticsearch": "^1.3.3",
    "@lando/mariadb": "^1.6.3",
    "@lando/memcached": "^1.3.3",
    "@lando/mysql": "^1.4.4",
    "@lando/php": "^1.7.1",
    "@lando/postgres": "^1.4.4",
    "@lando/redis": "^1.2.3",
    "@lando/varnish": "^1.3.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@lando/leia": "^1.0.0-beta.3",
    "@lando/vitepress-theme-default-plus": "^1.1.1",
    "chai": "^4.3.4",
    "command-line-test": "^1.0.10",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^9.21.1",
    "mocha": "^10.3.0",
    "nyc": "^15.1.0",
    "vitepress": "^1.3.4"
  }
}
